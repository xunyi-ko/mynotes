// 可以通过写中间页面来完成

// showUrl.wxml
<view class="page">
    <view class="page-body">
        <web-view src="{{url}}"></web-view>
    </view>
</view>

// showUrl.js
// pages/index/showUrl.js
//只需在自动生成的js文件中的data中加入url:''
//在onLoad中加入一点内容
Page({
  /**
   * 页面的初始数据
   */
  data: {
    url : ''
  },
  /**
   * 生命周期函数--监听页面加载
   */
  onLoad: function (options) {
    // 设置全局变量
    var that = this;

    var url = wx.getStorageSync('showUrl');
    that.setData({
      url: url
    })
  },
})

// 在有跳转需求的页面对应的js中加入方法
showUrl: function (e) {
    // 直接返回html
    wx.setStorageSync('showUrl', e.currentTarget.dataset.url)
    wx.navigateTo({
        // 注意：url的值需要根据当前文件夹情况进行修改
        url: 'showUrl'
    })
},

// 在需要跳转的地方写上 bindtap="showUrl" data-url="{{item.reprintUrl}}"
// 即可完成跳转外部页面
