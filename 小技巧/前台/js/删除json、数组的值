// 删除json的值
var json = {name : "zhangsan", age : "21", gender : "male"};
delete json[name];
console.log(json);

// 删除数组的值
var array = new Array();
array.push(1);
array.push(2);
array.splice(0, 1);
// 会删除掉下标为0的值，第二个参数为0时，不会删除。

// 查看元素是否为空
$.isEmptyObject(json); // false
delete json[age];
delete json[gender];
$.isEmptyObject(json); // 
